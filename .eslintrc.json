{
  "extends": ["airbnb", "prettier"],
  "plugins": ["react", "prettier", "import", "jsx-a11y"],
  "settings": {
    "import/core-modules": []
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
